#!/bin/bash

rm -fr SailfishWidgets
tar -xvjf ../../sailfish[-_]widgets/release/*.tar.bz2
find . -name "qmldir" -exec sed -i "s/module Sail/module harbour.opentip.Sail/" \{} \;
find . -name "*.qmltypes" -exec sed -i "s/SailfishWidgets/harbour\/opentip\/SailfishWidgets/" \{} \;
rm -fr SailfishWidgets/Documentation
